{
  "name": "touchbar-shortcuts",
  "displayName": "touchbar-shortcuts",
  "description": "Touchbar shortcuts for Vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "touchbar-shortcuts.rename",
        "title": "Rename"
      },
      {
        "command": "touchbar-shortcuts.fix-all-imports",
        "title": "Fix All Imports"
      },
      {
        "command": "touchbar-shortcuts.fix-imports",
        "title": "Fix Imports"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "touchbar-shortcuts.rename",
          "group": "editing"
        },
        {
          "command": "touchbar-shortcuts.fix-all-imports",
          "when": "dart-code:anyProjectLoaded",
          "group": "refactor"
        },
        {
          "command": "touchbar-shortcuts.fix-imports",
          "when": "dart-code:anyProjectLoaded",
          "group": "refactor"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.13",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.2.4",
    "vscode-test": "^1.5.2"
  }
}